<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="NewSpider" type="NewSpider.NewSpiderConfig,NewSpider"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  
  <NewSpider>
    <tools>
      <add key ="IsLog" value="true"></add>
      <add key ="Path" value="D:/Log"></add>
    </tools>
    <urls>
      <add key="google" url="http://www.google.com"  useproxy="true" uselocal="false" proxytype="1"/>
      <add key="sina" url="http://www.sina.com"   useproxy="true" uselocal="false" proxytype="1" />
      <add key="360buys" url="http://www.360buys.com"  useproxy="true" uselocal="false" proxytype="1" />
    </urls>
  </NewSpider>
</configuration>